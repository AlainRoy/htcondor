HFC_GAHP_SERVER_OBJ = hfcgahp_main.o hfcgahp_io.o hfcgahp_command.o hfcgahp_user_commands.o

public_c_plus_target($(HFC_GAHP_SERVER),sbin,$(HFC_GAHP_SERVER_OBJ),$(LIB))

