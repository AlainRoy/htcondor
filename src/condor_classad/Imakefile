all_target(libclassad.a)

C_PLUS_FLAGS = -Wall $(STD_C_PLUS_FLAGS)
CFLAGS = -Wall $(STD_C_FLAGS)

OBJ = ast.o astbase.o buildtable.o parser.o scanner.o registration.o \
	  attrlist.o classad.o classifiedjobs.o classad_util.o environment.o \
	  value.o operators.o evaluateOperators.o classad_instantiate.o

library_target(libclassad.a,$(OBJ))

c_plus_target(test, test.o, libclassad.a $(CUTIL_LIB) SocketLib)
pure_c_plus_target(test.pure, test.o, libclassad.a $(CUTIL_LIB) SocketLib)

clean::
	rm -f *.o libclassad.a

release:: all
stripped:: all
static:: all
html:
