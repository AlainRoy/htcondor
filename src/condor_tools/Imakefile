static_target(condor_qedit,qedit.o,$(LIB),$(STATIC_DIR)/bin)

c_plus_target(condor_transfer_data,transfer_data.o,$(LIB))
release_target(condor_transfer_data,bin,EXECUTABLE_MODE)
strip_target(condor_transfer_data,bin,EXECUTABLE_MODE)
static_target(condor_transfer_data,transfer_data.o,$(LIB),$(STATIC_DIR)/bin)

c_plus_target(condor_submit_dag,condor_submit_dag.o,$(LIB))

c_plus_target(condor_version,version.o,$(LIB))
release_target(condor_version,bin,EXECUTABLE_MODE)
strip_target(condor_version,bin,EXECUTABLE_MODE)
static_target(condor_version,version.o,$(LIB),$(STATIC_DIR)/bin)

c_plus_target(condor_wait,wait.o,$(LIB))
release_target(condor_wait,bin,EXECUTABLE_MODE)
strip_target(condor_wait,bin,EXECUTABLE_MODE)
static_target(condor_wait,wait.o,$(LIB),$(STATIC_DIR)/bin)

c_plus_target(condor,tool.o,$(LIB))
release_target(condor,sbin,EXECUTABLE_MODE)
release_copy_target($(RELEASE_DIR)/sbin/condor,bin,condor,EXECUTABLE_MODE)
strip_target(condor,sbin,EXECUTABLE_MODE)
strip_copy_target($(STRIP_DIR)/sbin/condor,bin,condor,EXECUTABLE_MODE)
static_target(condor,tool.o,$(LIB),$(STATIC_DIR)/sbin)
static_copy_target($(STATIC_DIR)/sbin/condor,$(STATIC_DIR)/bin,condor,EXECUTABLE_MODE)
purecov_c_plus_target(condor.pc,tool.o,$(LIB))
pure_c_plus_target(condor.pure,tool.o,$(LIB))

release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_on)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_off)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_checkpoint)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_vacate)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_restart)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_reconfig)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_reconfig_schedd)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_reschedule)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_master_off)

release_link_target($(RELEASE_DIR)/bin/condor,bin,condor_checkpoint)
release_link_target($(RELEASE_DIR)/bin/condor,bin,condor_vacate)
release_link_target($(RELEASE_DIR)/bin/condor,bin,condor_reschedule)

strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_on)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_off)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_checkpoint)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_vacate)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_restart)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_reconfig)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_reconfig_schedd)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_reschedule)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_master_off)

strip_link_target($(STRIP_DIR)/bin/condor,bin,condor_checkpoint)
strip_link_target($(STRIP_DIR)/bin/condor,bin,condor_vacate)
strip_link_target($(STRIP_DIR)/bin/condor,bin,condor_reschedule)

static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_on)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_off)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_checkpoint)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_vacate)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_restart)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_reconfig)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_reconfig_schedd)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_reschedule)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_master_off)

static_link_target($(STATIC_DIR)/bin/condor,bin,condor_checkpoint)
static_link_target($(STATIC_DIR)/bin/condor,bin,condor_vacate)
static_link_target($(STATIC_DIR)/bin/condor,bin,condor_reschedule)

c_plus_target(condor_stats,stats.o,$(LIB))
static_target(condor_stats,stats.o,$(LIB),$(STATIC_DIR)/bin)
strip_target(condor_stats,bin,EXECUTABLE_MODE)
release_target(condor_stats,bin,EXECUTABLE_MODE)

c_plus_target(test_calc,test_calc.o,$(LIB))

c_plus_target(view_collection,view_collection.o,$(LIB))

c_plus_target(condor_invalidate_host,invalidate_host.o,$(LIB))


condor_master_on: master_on.c
	gcc -I/usr/afsws/include -o condor_master_on master_on.c

condor_kill: kill.C
	g++ -o condor_kill kill.C -I../condor_c++_util -I../condor_includes

clean::
	rm -f condor_kill condor_master_on *.o

