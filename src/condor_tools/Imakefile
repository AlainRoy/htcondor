PROGRAMS =  	condor_config_val condor_preen condor_stats \
		condor_userprio condor_history condor test_calc \
		condor_release view_collection condor_release_license

all_target( $(PROGRAMS) )

CFLAGS = $(STD_C_FLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS)  
LDFLAGS = RpcLib SocketLib
LIB = $(STD_LIBS)

c_plus_target(condor_preen,preen.o,$(LIB))
release_target(condor_preen,$(RELEASE_DIR)/sbin,EXECUTABLE_MODE)
strip_target(condor_preen,$(STRIP_DIR)/sbin,EXECUTABLE_MODE)
static_target(condor_preen,preen.o,$(LIB),$(STATIC_DIR)/sbin)

c_plus_target(condor_release,releasejob.o, $(LIB))
release_target(condor_release,$(RELEASE_DIR)/bin,EXECUTABLE_MODE)
strip_target(condor_release,$(STRIP_DIR)/bin,EXECUTABLE_MODE)
static_target(condor_release,releasejob.o, $(LIB),$(STATIC_DIR)/bin)

c_plus_target(condor_config_val,config_val.o,$(LIB))
release_target(condor_config_val,$(RELEASE_DIR)/bin,EXECUTABLE_MODE)
strip_target(condor_config_val,$(STRIP_DIR)/bin,EXECUTABLE_MODE)
static_target(condor_config_val,config_val.o,$(LIB),$(STATIC_DIR)/bin)
purecov_c_plus_target(condor_config_val.pc,config_val.pure.o,$(LIB))

config_val.pure.o: config_val.C
	$(CPlusPlus) -o config_val.pure.o -c config_val.C -DPURE_DEBUG \
	$(C_PLUS_FLAGS) 

c_plus_target(condor_userprio,user_prio.o,$(LIB))
release_target(condor_userprio,$(RELEASE_DIR)/bin,EXECUTABLE_MODE)
strip_target(condor_userprio,$(STRIP_DIR)/bin,EXECUTABLE_MODE)
static_target(condor_userprio,user_prio.o,$(LIB),$(STATIC_DIR)/bin)

c_plus_target(condor_history,history.o,$(LIB))
release_target(condor_history,$(RELEASE_DIR)/bin,EXECUTABLE_MODE)
strip_target(condor_history,$(STRIP_DIR)/bin,EXECUTABLE_MODE)
static_target(condor_history,history.o,$(LIB),$(STATIC_DIR)/bin)

c_plus_target(condor,tool.o,$(LIB))
release_target(condor,$(RELEASE_DIR)/sbin,EXECUTABLE_MODE)
strip_target(condor,$(STRIP_DIR)/sbin,EXECUTABLE_MODE)
static_target(condor,tool.o,$(LIB),$(STATIC_DIR)/sbin)
purecov_c_plus_target(condor.pc,tool.o,$(LIB))

release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_on)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_off)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_checkpoint)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_vacate)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_restart)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_reconfig)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_reconfig_schedd)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_reschedule)
release_link_target($(RELEASE_DIR)/sbin/condor,sbin,condor_master_off)

release_link_target($(RELEASE_DIR)/sbin/condor,bin,condor_checkpoint)
release_link_target($(RELEASE_DIR)/sbin/condor,bin,condor_vacate)
release_link_target($(RELEASE_DIR)/sbin/condor,bin,condor_reschedule)

strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_on)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_off)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_checkpoint)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_vacate)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_restart)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_reconfig)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_reconfig_schedd)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_reschedule)
strip_link_target($(STRIP_DIR)/sbin/condor,sbin,condor_master_off)

strip_link_target($(STRIP_DIR)/sbin/condor,bin,condor_checkpoint)
strip_link_target($(STRIP_DIR)/sbin/condor,bin,condor_vacate)
strip_link_target($(STRIP_DIR)/sbin/condor,bin,condor_reschedule)

static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_on)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_off)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_checkpoint)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_vacate)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_restart)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_reconfig)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_reconfig_schedd)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_reschedule)
static_link_target($(STATIC_DIR)/sbin/condor,sbin,condor_master_off)

static_link_target($(STATIC_DIR)/sbin/condor,bin,condor_checkpoint)
static_link_target($(STATIC_DIR)/sbin/condor,bin,condor_vacate)
static_link_target($(STATIC_DIR)/sbin/condor,bin,condor_reschedule)

c_plus_target(condor_stats,stats.o,$(LIB))
static_target(condor_stats,stats.o,$(LIB),$(STATIC_DIR)/sbin)
strip_target(condor_stats,$(STRIP_DIR)/sbin,EXECUTABLE_MODE)
release_target(condor_stats,$(RELEASE_DIR)/sbin,EXECUTABLE_MODE)

c_plus_target(condor_release_license,release_license.o,$(LIB))
static_target(condor_release_license,release_license.o,$(LIB),$(STATIC_DIR)/sbin)
strip_target(condor_release_license,$(STRIP_DIR)/sbin,EXECUTABLE_MODE)
release_target(condor_release_license,$(RELEASE_DIR)/sbin,EXECUTABLE_MODE)

c_plus_target(test_calc,test_calc.o,$(LIB))

c_plus_target(view_collection,view_collection.o,$(LIB))

condor_master_on: master_on.c
	gcc -o condor_master_on master_on.c

condor_kill: kill.C
	g++ -o condor_kill kill.C -I../condor_c++_util

clean::
	rm -f condor_kill condor_master_on *.o

