PROGRAMS =  	condor_config_val condor_preen condor_stats \
		condor_userprio condor_history condor test_calc condor_transfer_data \
		view_collection condor_qedit condor_advertise condor_version \
		condor_fetchlog condor_submit_dag condor_findhost condor_cod condor_wait

all_target( $(PROGRAMS) )

CFLAGS = $(STD_C_FLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS)  
LIB = $(STD_LIBS)

c_plus_target(condor_cod,cod_tool.o,$(LIB))
release_target(condor_cod,bin,EXECUTABLE_MODE)
strip_target(condor_cod,bin,EXECUTABLE_MODE)
static_target(condor_cod,cod_tool.o,$(LIB),$(STATIC_DIR)/bin)

c_plus_target(condor_preen,preen.o,$(LIB))
release_target(condor_preen,sbin,EXECUTABLE_MODE)
strip_target(condor_preen,sbin,EXECUTABLE_MODE)
static_target(condor_preen,preen.o,$(LIB),$(STATIC_DIR)/sbin)
pure_c_plus_target(condor_preen.pure,preen.o,$(LIB))

c_plus_target(condor_advertise,advertise.o,$(LIB))
release_target(condor_advertise,sbin,EXECUTABLE_MODE)
strip_target(condor_advertise,sbin,EXECUTABLE_MODE)
static_target(condor_advertise,advertise.o,$(LIB),$(STATIC_DIR)/sbin)
pure_c_plus_target(condor_advertise.pure,advertise.o,$(LIB))

c_plus_target(condor_fetchlog,fetch_log.o,$(LIB))
release_target(condor_fetchlog,sbin,EXECUTABLE_MODE)
strip_target(condor_fetchlog,sbin,EXECUTABLE_MODE)
static_target(condor_fetchlog,fetch_log.o,$(LIB),$(STATIC_DIR)/sbin)
pure_c_plus_target(condor_fetchlog.pure,fetch_log.o,$(LIB))

c_plus_target(condor_config_val,config_val.o,$(LIB))
release_target(condor_config_val,bin,EXECUTABLE_MODE)
strip_target(condor_config_val,bin,EXECUTABLE_MODE)
static_target(condor_config_val,config_val.o,$(LIB),$(STATIC_DIR)/bin)
purecov_c_plus_target(condor_config_val.pc,config_val.pure.o,$(LIB))
pure_c_plus_target(condor_config_val.pure,config_val.pure.o,$(LIB))

config_val.pure.o: config_val.C
	$(CPlusPlus) -o config_val.pure.o -c config_val.C -DPURE_DEBUG \
	$(C_PLUS_FLAGS) 

c_plus_target(condor_userprio,user_prio.o,$(LIB))
release_target(condor_userprio,bin,EXECUTABLE_MODE)
strip_target(condor_userprio,bin,EXECUTABLE_MODE)
static_target(condor_userprio,user_prio.o,$(LIB),$(STATIC_DIR)/bin)

c_plus_target(condor_findhost,give_interactive.o,$(LIB))
release_target(condor_findhost,bin,EXECUTABLE_MODE)
strip_target(condor_findhost,bin,EXECUTABLE_MODE)
static_target(condor_findhost,give_interactive.o,$(LIB),$(STATIC_DIR)/bin)

c_plus_target(condor_history,history.o,$(LIB))
release_target(condor_history,bin,EXECUTABLE_MODE)
strip_target(condor_history,bin,EXECUTABLE_MODE)
static_target(condor_history,history.o,$(LIB),$(STATIC_DIR)/bin)

