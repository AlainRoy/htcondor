all_target(libacct.a condor_accountant)

C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS)
CFLAGS = $(STD_C_FLAGS)
LIB = libacct.a $(DAEMONCORE_LIB) $(STD_LIBS) $(IO_LIB)

LIB_OBJ = Accountant.o
OBJ = main.o

library_target(libacct.a, $(LIB_OBJ))

c_plus_target(condor_accountant,$(OBJ),$(LIB))
pure_c_plus_target(condor_accountant.pure,$(OBJ),$(LIB))

