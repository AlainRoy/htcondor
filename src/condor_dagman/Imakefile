NAME = condor_dagman
all_target($(NAME))

C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) -Wall
CFLAGS = $(STD_C_FLAGS) -Wall
LDFLAGS = SocketLib
LIB = $(DAEMONCORE_LIB) $(CUTIL_LIB) $(CPLUS_LIB) \
      ../condor_classad.V6/libclassad.a $(IO_LIB) $(CUTIL_LIB) $(CPLUS_LIB)

OBJ = parser.o util.o types.o debug.o script.o submit.o parse.o job.o dag.o main.o instantiate.o

c_plus_target($(NAME),$(OBJ),$(LIB))
release_target($(NAME),$(RELEASE_DIR)/bin,EXECUTABLE_MODE)
strip_target($(NAME),$(STRIP_DIR)/bin,EXECUTABLE_MODE)
static_target($(NAME),$(OBJ),$(LIB),$(STATIC_DIR)/bin)
purecov_c_plus_target($(NAME).pc,$(OBJ),$(LIB))
pure_c_plus_target($(NAME).pure,$(OBJ),$(LIB))

html_target( dag.h debug.h job.h script.h submit.h types.h util.h )

release_target(condor_submit_dag,$(RELEASE_DIR)/bin,EXECUTABLE_MODE)
strip_copy_target(condor_submit_dag,$(STRIP_DIR)/bin, \
		condor_submit_dag,EXECUTABLE_MODE)
static_copy_target(condor_submit_dag,$(STATIC_DIR)/bin, \
		condor_submit_dag,EXECUTABLE_MODE)
