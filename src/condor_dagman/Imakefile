#if WANT_NEW_CLASSADS
#define New_classads_defines    -DWANT_NEW_CLASSADS
#define New_classads_lib		-L$(NEW_CLASSADS_LIB) -lclassad_ns
#else
#define New_classads_defines
#define New_classads_lib
#endif

NAME = condor_dagman
all_target($(NAME) t_helper condor_submit_dag)

#if defined(DAGMAN_HELPER)
HELPER_FLAGS = -DBUILD_HELPER
#endif

C_PLUS_FLAGS = -I. $(STD_C_PLUS_FLAGS) \
	-I../condor_daemon_core.V6 -I.. \
	New_classads_defines \
	-Wall $(HELPER_FLAGS)

CFLAGS = $(STD_C_FLAGS) New_classads_defines
LIB = $(DAEMONCORE_LIB) $(STD_LIBS) New_classads_lib
TOOL_LIB = $(STD_LIBS) New_classads_lib

soapobjs(dagman)

OBJ = dagman_main.o submit.o dag.o job.o util.o parse.o debug.o \
	script.o scriptQ.o helper.o  dagman_commands.o \
	dagman_multi_dag.o $(SOAP_OBJS)

public_c_plus_target($(NAME),bin,$(OBJ),$(LIB))

public_c_plus_target(condor_submit_dag,bin,condor_submit_dag.o dagman_multi_dag.o,$(TOOL_LIB))

html_target( documentation )

#if defined(DAGMAN_HELPER)
c_plus_target(t_helper,t_helper.o helper.o,$(LIB))
#endif

gen_soapfiles(dagman,condorDagman)
