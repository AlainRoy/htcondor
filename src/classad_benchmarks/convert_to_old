#! /usr/bin/env python
import sys

class AdLines ( object ) :
    def __init__( self ) :
        self._lines = []
    def Add( self, line ) :
        line = line.strip()
        if len(line) == 0 :
            return False
        else :
            self._lines.append( line )
            return True
    def Print( self ) :
        if len(self._lines) == 0 :
            return
        for l in self._lines :
            print l
        print ";"
    def Reset( self ) :
        self._lines = []

lines = AdLines()
for line in sys.stdin :
    if not lines.Add(line) :
        lines.Print()
        lines.Reset()
lines.Print()

