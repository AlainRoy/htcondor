NAME = condor_transferd
all_target( $(NAME) )

CFLAGS = $(STD_C_FLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS)
LIB = $(DAEMONCORE_LIB) $(STD_LIBS) $(CONFIGURE_GSOAP_LDFLAGS)

soapobjs(transferd)

OBJ =	td_main.o \
		td_init.o \
		td_maint.o \
		td_utils.o \
		td_write_files.o \
		td_read_files.o \
		td_active.o \
		td_instantiate.o \
		$(SOAP_OBJS)

template_inst(td_instantiate.C,td_instantiate.o)

public_c_plus_target($(NAME),sbin,$(OBJ),$(LIB))

