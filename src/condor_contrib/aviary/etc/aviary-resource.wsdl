<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://grid.redhat.com/aviary-resource/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:res="http://resource.aviary.grid.redhat.com" xmlns:ns="http://common.aviary.grid.redhat.com" targetNamespace="http://grid.redhat.com/aviary-resource/" name="aviary-resource">
	<wsdl:types>
		<xsd:schema targetNamespace="http://grid.redhat.com/aviary-resource/">
			<xsd:import schemaLocation="aviary-resource.xsd" namespace="http://resource.aviary.grid.redhat.com"/>
		</xsd:schema>
	</wsdl:types>
	<wsdl:message name="GetAttributesRequest">
		<wsdl:part name="parameters" element="res:GetAttributes"/>
	</wsdl:message>
	<wsdl:message name="GetAttributesResponse">
		<wsdl:part name="parameters" element="res:GetAttributesResponse"/>
	</wsdl:message>
	<wsdl:portType name="AviaryResource">
		<wsdl:operation name="getAttributes">
			<wsdl:input message="tns:GetAttributesRequest"/>
			<wsdl:output message="tns:GetAttributesResponse"/>
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="AviaryResourceSOAPBinding" type="tns:AviaryResource">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="getAttributes">
			<soap:operation soapAction="http://grid.redhat.com/aviary-resource/attributes"/>
			<wsdl:input>
				<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="AviaryResourceService">
		<wsdl:port name="AviaryResourceSOAPPort" binding="tns:AviaryResourceSOAPBinding">
			<soap:address location="http://localhost"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
