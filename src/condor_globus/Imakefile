NAME=condor_gridmanager
all_target($(NAME) condor_glidein)

CFLAGS = $(STD_C_FLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS)
LIB = $(DAEMONCORE_LIB) $(STD_LIBS)

OBJ = gridmanager_main.o gridmanager.o globusjob.o

c_plus_target($(NAME),$(OBJ),$(LIB))

release_target($(NAME),$(RELEASE_DIR)/sbin,EXECUTABLE_MODE)
strip_target($(NAME),$(STRIP_DIR)/sbin,EXECUTABLE_MODE)
static_target($(NAME),$(OBJ),$(LIB),$(STATIC_DIR)/sbin)
purecov_c_plus_target($(NAME).pc,$(OBJ),$(LIB))
pure_c_plus_target($(NAME).pure,$(OBJ),$(LIB))

release_copy_target(condor_glidein,bin,condor_glidein,EXECUTABLE_MODE)
strip_copy_target(condor_glidein,$(STRIP_DIR)/bin,condor_glidein,EXECUTABLE_MODE)
static_copy_target(condor_glidein,$(STATIC_DIR)/bin,condor_glidein,EXECUTABLE_MODE)

html:
