NAME = condor_submit
all_target($(NAME))

#if IS_CLIPPED
CLIPPED = -DCLIPPED
#endif

CFLAGS = $(STD_C_FLAGS) $(CLIPPED) -I$(NEW_CLASSADS_INC)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) $(CLIPPED)  -I$(NEW_CLASSADS_INC)
LIB = $(STD_LIBS) -L$(NEW_CLASSADS_LIB) -lclassad_ns

OBJ = submit.o

public_c_plus_target($(NAME),bin,$(OBJ),$(LIB))

