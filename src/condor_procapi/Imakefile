all_target(libprocapi.a)

C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS)
CFLAGS = $(STD_C_FLAGS)
LIB = $(STD_LIBS) ../condor_classad/libclassad.a ../condor_io/libcedar.a

OBJ = procapi.o procinterface.o procapi_instantiate.o

library_target(libprocapi.a,$(OBJ))

c_plus_target(procapi.tester,procapi.t.o,$(LIB))
pure_c_plus_target(procapi.tester.pure,procapi.t.o,$(LIB))
c_plus_target(nt-testprocapi, testprocapi.o, $(LIB))
pure_c_plus_target(nt-testprocapi.pure, testprocapi.o, $(LIB))

html_target( procapi.h procinterface.h )

