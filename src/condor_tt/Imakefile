NAME = condor_tt

all_target( $(NAME))


CFLAGS = $(STD_C_FLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS)
LDFLAGS = $(STD_LDFLAGS)
LIB = $(DAEMONCORE_LIB) $(STD_LIBS)

OBJ = tt_main.o ttmanager.o

c_plus_target($(NAME),$(OBJ),$(LIB))

release_target($(NAME),sbin,EXECUTABLE_MODE)

