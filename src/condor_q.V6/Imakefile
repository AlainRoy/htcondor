CFLAGS = $(STD_C_FLAGS) $(PLATFORM_C_FLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) -DNEW_PROC -fno-implicit-templates
LDFLAGS = -lm RpcLib SocketLib

SRC = queue.C
OBJ = queue.o
LIB = ../condor_c++_util/cplus_lib.a ../condor_schedd.V6/libqmgmt.a ../condor_c++_util/cplus_lib.a ../condor_classad/expr_lib.a ../condor_util_lib/util_lib.a $(AUTH2_LIB)
LIBDIRS= ../condor_util_lib ../condor_c++_util ../condor_classad

all_target(condor_q)
tags_target($(SRC),$(LIBDIRS))
depend_target($(SRC))
c_plus_target(condor_q,$(OBJ),$(LIB))
release_target(condor_q,$(RELEASE_DIR)/bin,EXECUTABLE_MODE)
strip_target(condor_q,$(STRIP_DIR)/bin,EXECUTABLE_MODE)
static_target(condor_q,$(OBJ),$(LIB),$(STATIC_DIR)/bin)
