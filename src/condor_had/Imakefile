NAME = condor_had
all_target($(NAME))

CFLAGS = $(STD_C_FLAGS) 
C_PLUS_FLAGS =  $(STD_C_PLUS_FLAGS)
LIB = $(DAEMONCORE_LIB) $(STD_LIBS)

OBJ =  StateMachine.o HAD.o


c_plus_target(condor_had,$(OBJ),$(LIB))
release_target(condor_had,sbin,EXECUTABLE_MODE)
strip_target(condor_had,sbin,EXECUTABLE_MODE)
static_target(condor_had,$(OBJ),$(LIB),$(STATIC_DIR)/sbin)
pure_c_plus_target(condor_had.pure,$(OBJ),$(LIB))
purecov_c_plus_target(condor_had.pc,$(OBJ),$(LIB))

