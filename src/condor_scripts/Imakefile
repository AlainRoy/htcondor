SRC_DIR = $(SRC_TREE)/condor_scripts
CFLAGS = $(STD_C_FLAGS)

SRC =

TARGETS = \
	condor_config config_on config_off condor_throttle daemons_off condor_init

all_target($(TARGETS))
depend_target($(SRC))
tags: ALWAYS

#if IS_ALPHA_OSF1
text_target(condor_config,lib,condor_config.generic)
text_target(config_on,lib,config_on.generic)
text_target(config_off,lib,config_off.generic)
text_target(daemons_off,lib,daemons_off)
script_target(condor_throttle,bin,condor_throttle.generic,SET_GID_MODE)
script_target(condor_init,bin,condor_init.generic,EXECUTABLE_MODE)
#else
text_target(condor_config,lib)
text_target(config_on,lib)
text_target(config_off,lib)
text_target(daemons_off,lib)
script_target(condor_throttle,bin)
script_target(condor_init,bin)
setgid_target(condor_throttle)
#endif
